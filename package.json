{
  "name": "itrex-lab-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "server": "export NODE_ENV=dev && node index.js",
    "websocket": "export NODE_ENV=dev && node ./src/websocketServer/server.js",
    "server_dev": "export NODE_ENV=dev && nodemon index.js",
    "websocket_dev": "export NODE_ENV=dev && node ./src/websocketServer/server.js",
    "server_debug": "export NODE_ENV=dev && node inspect index.js",
    "websocket_debug": "export NODE_ENV=dev && node ./src/websocketServer/server.js",
    "prod": "export NODE_ENV=prod && node index.js & node ./src/scheduler/index.js",
    "dev": "concurrently --kill-others 'npm run server_dev' 'npm run websocket_dev'",
    "debug": "concurrently --kill-others 'npm run server_debug' 'npm run websocket_debug'",
    "start": "concurrently --kill-others 'npm run server' 'npm run websocket' ",
    "lint": "eslint src/ --ext .js",
    "test": "export NODE_ENV=dev && mocha test.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12",
    "process": "^0.11.10"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "cache": "^2.3.1",
    "chai": "^4.3.4",
    "chalk": "^3.0.0",
    "clean": "^4.0.2",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.0",
    "node-redis-scan": "^1.3.3",
    "npm": "^7.20.5",
    "passport": "^0.4.1",
    "redis": "^3.1.2",
    "sequelize": "^6.6.5",
    "uuid": "^8.3.2",
    "validator": "^13.6.0"
  }
}
